{
    "name": "atypiccraft/string-blade-compiler",
    "description": "Laravel Blade String Compiler, render string as blade templates. Fork of wpb/string-blade-compiler",
    "keywords": ["laravel", "blade", "compiler"],
    "license": "MIT",
    "authors": [
        {
          "name": "Garrett Dengler",
          "email": "garrett@atypiccraft.com"
        },
        {
            "name": "Terre Porter (original)",
            "email": "tporter@webpage-builders.com"
        }
    ],
    "require": {
        "php": "^7.2|^8",
        "laravel/framework": "^6|^7|^8"
    },
    "autoload": {
        "psr-4": {
            "Atypiccraft\\StringBladeCompiler\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Atypiccraft\\StringBladeCompiler\\StringBladeServiceProvider"
            ],
            "aliases": {
                "StringBlade": "Atypiccraft\\StringBladeCompiler\\Facades\\StringBlade"
            }
        }
    }
}
